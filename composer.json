{
    "name": "ayles-software/laravel-sms-mobile-message",
    "description": "MobileMessage Notifications channel for Laravel 12",
    "keywords": ["laravel", "notifications", "mobile-message", "sms"],
    "homepage": "https://github.com/ayles-software/laravel-sms-mobile-message",
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Ayles",
            "role": "Developer",
            "email": "ben@aylessoftware.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/http": "^10.0||^11.0||^12.0",
        "illuminate/events": "^10.0||^11.0||^12.0",
        "illuminate/support": "^10.0||^11.0||^12.0",
        "illuminate/notifications": "^10.0||^11.0||^12.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "AylesSoftware\\MobileMessage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\ClickSend\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "cs-fix": "vendor/bin/php-cs-fixer fix"
    }
}
